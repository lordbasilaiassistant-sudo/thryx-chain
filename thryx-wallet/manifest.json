{
  "manifest_version": 3,
  "name": "THRYX Wallet",
  "version": "1.0.0",
  "description": "Official wallet for the THRYX blockchain - AI-native L2",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
